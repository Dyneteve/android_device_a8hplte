/system       ext4  /dev/block/mmcblk0p25
/system_image ext4  /dev/block/mmcblk0p25
/cache        ext4  /dev/block/mmcblk0p26
/data         ext4  /dev/block/mmcblk0p27
/persist      ext4  /dev/block/mmcblk0p22  flags=display="Persist";backup=1
/efs          ext4  /dev/block/mmcblk0p13  flags=display="EFS";backup=1
